<div class="text-center">
    <label class="display-5">Exceptions</label>
</div>

<table id="exception-datatable" class="table table-striped table-bordered dt-responsive nowrap" width="100%">
    <thead>
        <tr>
            <th> Id </th>
            <th> Application Name </th>
            <th> Create Date </th>
            <th> Ocurred At </th>
            <th> Message </th>
            <th> Type Of </th>
            <th> Source </th>
            <th> Path </th>
            <th> Method </th>
            <th> Query String </th>
            <th> Request Body </th>
            <th> Stack Trace </th>
            <th> Request </th>
            <th> Response </th>
            <th> Log </th>
        </tr>
    </thead>
</table>
<div id="exeception-modal-query-string" class="modalDialog">
    <div>
        <a href="#close" title="Close" class="close">X</a>
        <h2>Modal Box</h2>
        <p>Hello world</p>
    </div>
</div>
@section Scripts {
    <script type="module" src="~/js/tables/exception.js" asp-append-version="true"></script>
}